  "ocamlbuild" {= "0.14.3"}
  "ocamlc-loc" {= "3.14.2" & with-dev-setup}
  "ocamlfind" {= "1.9.6"}
  "ocamlformat" {= "0.26.1" & with-dev-setup}
  "ocamlformat-lib" {= "0.26.1" & with-dev-setup}
  "ocamlformat-rpc-lib" {= "0.26.1" & with-dev-setup}
  "ocp-indent" {= "1.8.1" & with-dev-setup}
  "optint" {= "0.3.0"}
  "ordering" {= "3.14.2" & with-dev-setup}
  "pinc-lang" {= "dev"}
  "pp" {= "1.2.0" & with-dev-setup}
  "ppx_yojson_conv_lib" {= "v0.16.0" & with-dev-setup}
  "psq" {= "0.2.1"}
  "ptime" {= "1.1.0"}
  "re" {= "1.11.0" & with-dev-setup}
  "result" {= "1.5" & with-dev-setup}
  "riot" {= "0.0.5"}
  "rresult" {= "0.7.0"}
  "seq" {= "base"}
  "sexplib0" {= "v0.16.0" & with-dev-setup}
  "spawn" {= "v0.15.1" & with-dev-setup}
  "stdio" {= "v0.16.0" & with-dev-setup}
  "stdlib-shims" {= "0.3.0"}
  "stdune" {= "3.14.2" & with-dev-setup}
  "stringext" {= "1.6.0"}
  "telemetry" {= "0.0.1"}
  "thread-table" {= "1.0.0"}
  "topkg" {= "1.0.7"}
  "tty" {= "0.0.2"}
  "uri" {= "4.4.0"}
  "uucp" {= "15.1.0" & with-dev-setup}
  "uuseg" {= "15.1.0" & with-dev-setup}
  "uutf" {= "1.0.3"}
  "xdg" {= "3.14.2" & with-dev-setup}
  "yaml" {= "3.2.0"}
  "yojson" {= "2.1.2" & with-dev-setup}
]
build: [
  ["dune" "subst"] {dev}
  [
    "dune"
    "build"
    "-p"
    name
    "-j"
    jobs
    "@install"
    "@runtest" {with-test}
    "@doc" {with-doc}
  ]
]
dev-repo: "git+https://github.com/pinc-official/pinc-static.git"
pin-depends: [
  "pinc-lang.dev" "git+https://github.com/pinc-official/pinc-lang#main"
]