  "chrome-trace" {= "3.14.2" & with-dev-setup}
  "cmdliner" {= "1.2.0"}
  "conf-bash" {= "1" & with-dev-setup}
  "containers" {= "3.13.1"}
  "cppo" {= "1.6.9" & with-dev-setup}
  "crunch" {= "3.3.1"}
  "csexp" {= "1.5.2"}
  "cstruct" {= "6.2.0"}
  "ctypes" {= "0.22.0"}
  "domain-local-await" {= "1.0.1"}
  "dune" {= "3.14.2"}
  "dune-build-info" {= "3.14.2" & with-dev-setup}
  "dune-configurator" {= "3.14.2"}
  "dune-rpc" {= "3.14.2" & with-dev-setup}
  "dyn" {= "3.14.2" & with-dev-setup}
  "eio" {= "1.0"}
  "eio_main" {= "1.0"}
  "eio_posix" {= "1.0"}
  "either" {= "1.0.0"}
  "fiber" {= "3.7.0" & with-dev-setup}
  "fix" {= "20230505" & with-dev-setup}
  "fmt" {= "0.9.0"}
  "fpath" {= "0.7.3"}
  "hmap" {= "0.8.1"}
  "integers" {= "0.7.0"}
  "iomux" {= "0.3"}
  "logs" {= "0.7.0"}
  "lwt-dllist" {= "1.0.1"}
  "menhir" {= "20231231" & with-dev-setup}
  "menhirCST" {= "20231231" & with-dev-setup}
  "menhirLib" {= "20231231" & with-dev-setup}
  "menhirSdk" {= "20231231" & with-dev-setup}
  "merlin-lib" {= "4.14-501" & with-dev-setup}
  "minttea" {= "0.0.2"}
  "mtime" {= "2.0.0"}
  "ocaml" {= "5.1.1"}
  "ocaml-config" {= "3"}
  "ocaml-lsp-server" {= "1.17.0" & with-dev-setup}
  "ocaml-option-flambda" {= "1"}
  "ocaml-syntax-shims" {= "1.0.0"}
  "ocaml-variants" {= "5.1.1+options"}
